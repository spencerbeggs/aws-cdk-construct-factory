{
  "name": "aws-cdk-construct-factory",
  "version": "0.0.0",
  "description": "A bootstrap-able GitHub template for developing, testing and publishing AWS CDK constructs with a Node.js cli.",
  "author": {
    "name": "C. Spencer Beggs",
    "email": "spencer@beg.gs",
    "url": "https://github.com/spencerbeggs",
    "organization": false
  },
  "repository": {
    "url": "https://github.com/spencerbeggs/aws-cdk-construct-factory.git",
    "type": "git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "aws-cdk-construct-factory",
        "module": "aws-cdk-construct-factory"
      },
      "java": {
        "package": "spencerbeggs.aws_cdk_construct_factory",
        "maven": {
          "groupId": "spencerbeggs",
          "artifactId": "aws-cdk-construct-factory"
        }
      },
      "dotnet": {
        "namespace": "spencerbeggs",
        "packageId": "aws-cdk-construct-factory"
      }
    }
  },
  "devDependencies": {
    "jsii": "^1.2.0",
    "jsii-pacmak": "^1.2.0"
  }
}
